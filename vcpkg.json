{
  "name": "gtsplugin",
  "version-string": "2.0.0",
  "description": "Common Lib SSE plugin for Size Matters.",
  "features": {
    "plugin": {
      "description": "Build the SKSE plugin.",
      "dependencies": [
	    {
			"name": "imgui",
			"features": ["dx11-binding", "win32-binding", "docking-experimental"]
		},
        "commonlibsse-ng",
        "glm",
        "toml11",
        "nlohmann-json",
		"gluino"
      ]
    }
  },
  "default-features": ["plugin"],
  "overrides": [
    {
      "name" : "imgui",
      "version": "1.90"
    }
  ]
}
