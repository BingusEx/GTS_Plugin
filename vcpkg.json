{
  "$schema": "https://raw.githubusercontent.com/microsoft/vcpkg-tool/main/docs/vcpkg.schema.json",
  "name": "gtsplugin",
  "version-string": "2.0.0",
  "description": "SKSE plugin for Size Matters",
  "features": {
    "plugin": {
      "description": "Build the plugin.",
      "dependencies": [
		    "clib-util",
        "commonlibsse-ng",
        "glm",
        "toml11",
        "nlohmann-json",
		    "gluino",
        "fmt",
        "spdlog",
        "magic-enum",
        "boost-pfr"
      ]
    }
  },
  "default-features": ["plugin"],
  "overrides": [
	  {
      "name" : "toml11",
      "version": "4.3.0"
    },
	  {
      "name" : "nlohmann-json",
      "version": "3.11.3"
    }
  ]
}
